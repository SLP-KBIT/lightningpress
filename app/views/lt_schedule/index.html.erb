<p>lt_schedule</p>

<div id="calendar" >
  <script>
    $(document).ready(function() {
      var date = new Date();
      var d = date.getDate();
      var m = date.getMonth();
      var y = date.getFullYear();
      $('#calendar').fullCalendar({
        editable: false,
        header: {
          left: 'prev,next today',
          center: 'title',
          right: 'month,basicWeek,basicDay'
        },
        defaultView: 'month',
        events: [
          {
            title: 'SLP紹介',
            start: new Date(y, m, 1),
            url: 'http://google.com/'
          }
        ]
      })
    });
  </script>
</div>
